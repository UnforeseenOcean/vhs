  "devDependencies": {
    "babel-core": "^5.6.5",
    "babel-loader": "^5.1.4",
    "basscss-highlight-dark": "0.0.1",
    "blk": "^3.1.0",
    "css-loader": "^0.15.1",
    "cssnext": "^1.1.0",
    "cssnext-loader": "^1.0.1",
    "highlight.js": "^8.6.0",
    "json-loader": "^0.5.2",
    "lodash": "^3.9.3",
    "marked": "^0.3.3",
